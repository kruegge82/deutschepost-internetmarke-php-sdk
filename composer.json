{
    "name": "kruegge82/deutschepost-internetmarke-php-sdk",
    "description": "Division: Post & Parcel Germany<br /> The INTERNETMARKE is the online-postage for mail products of Deutsche Post AG.",
    "keywords": [
        "php",
        "sdk",
        "Deutsche Post",
        "api",
        "Internetmarke"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jahn N."
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "kruegge82\\DPIM\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "kruegge82\\DPIM\\Test\\" : "test/" }
    }
}
